<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppThemes" parent="Theme.MaterialComponents.Light">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="colorControlActivated">?colorPrimary</item>
        <item name="toolbarStyle">@style/Widget.Toolbar</item>
        <item name="tabStyle">@style/Widget.OPLApps.Tabs</item>
        <item name="android:textViewStyle">@style/Widget.OPLApps.TextView</item>
        <item name="android:windowBackground">@color/white</item>
        <item name="android:statusBarColor">@color/status_bar</item>
        <!--<item name="android:windowLightStatusBar" tools:targetApi="m">false</item>-->
        <item name="android:navigationBarColor">@color/colorPrimaryDark</item>
        <item name="android:textColorTertiary">@color/text_color_tertiary</item>
        <item name="android:actionBarStyle">@style/AppThemes.ActionBarStyle</item>
    </style>

    <style name="AppThemes.ActionBarStyle" parent="@android:style/Widget.Holo.Light.ActionBar">
        <item name="android:titleTextStyle">@style/AppThemes.ActionBar.TitleTextStyle</item>
    </style>

    <style name="AppThemes.ActionBar.TitleTextStyle" parent="@android:style/TextAppearance.Holo.Widget.ActionBar.Title">
        <item name="android:fontFamily">@font/google_sans</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/md_white</item>
    </style>

    <style name="AppThemes.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppThemes.LightNav" parent="AppThemes.NoActionBar" />

    <style name="AppThemes.Onboarding" parent="AppThemes.LightNav">
        <item name="android:windowEnterAnimation">@android:anim/fade_in</item>
        <item name="android:windowExitAnimation">@android:anim/fade_out</item>
    </style>

    <style name="AppThemes.PopupTheme" parent="ThemeOverlay.MaterialComponents.Light" />

    <style name="Widget.AppBar" parent="@style/Widget.Design.AppBarLayout">
        <item name="popupTheme">@style/AppThemes.PopupTheme</item>
        <item name="android:background">@color/white</item>
        <item name="android:elevation">8dp</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="Widget.Toolbar" parent="Widget.AppCompat.Toolbar">
        <item name="titleTextAppearance">@style/TextAppearance.OPLApps.ToolbarTitle</item>
        <item name="popupTheme">@style/AppThemes.PopupTheme</item>
    </style>

    <style name="TextAppearance.OPLApps.Body" parent="@style/TextAppearance.AppCompat.Body1">
        <item name="android:textColor">?android:textColorSecondary</item>
    </style>

    <style name="TextAppearance.OPLApps.ToolbarTitle" parent="@style/TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:fontFamily">@font/google_sans</item>
    </style>

    <style name="TextAppearance.OPLApps.SessionTitle" parent="@style/TextAppearance.AppCompat.Subhead">
        <item name="android:fontFamily">@font/google_sans</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="TextAppearance.OPLApps.ListSecondary" parent="@style/TextAppearance.AppCompat.Body1">
        <item name="android:textColor">?android:textColorSecondary</item>
    </style>

    <style name="TextAppearance.OPLApps.ReservationStatus" parent="@style/TextAppearance.OPLApps.ListSecondary">
        <item name="android:textAllCaps">true</item>
    </style>

    <style name="TextAppearance.OPLApps.Tag" parent="@style/TextAppearance.OPLApps.ListSecondary" />

    <style name="TextAppearance.OPLApps.AgendaTitle" parent="@style/TextAppearance.AppCompat.Body1">
        <item name="android:fontFamily">@font/google_sans</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="TextAppearance.OPLApps.SessionTitle.Display" parent="@style/TextAppearance.AppCompat.Display1">
        <item name="android:fontFamily">@font/google_sans</item>
        <item name="android:textColor">?android:textColorPrimary</item>
    </style>

    <style name="TextAppearance.OPLApps.Tab" parent="@style/TextAppearance.MaterialComponents.Tab">
        <item name="android:fontFamily">@font/google_sans</item>
        <item name="android:textColor">@color/tab_text</item>
        <item name="android:textStyle">bold</item>
        <item name="textAllCaps">false</item>
    </style>

    <style name="TextAppearance.OPLApps.BottomNavigationView" parent="@style/Widget.MaterialComponents.BottomNavigationView.Colored">
        <item name="android:fontFamily">@font/google_sans</item>
        <item name="android:textColor">@color/bottom_nav_item</item>
    </style>

    <style name="TextAppearance.OPLApps.BottomNavigationView.Active">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="TextAppearance.OPLApps.H1" parent="@style/TextAppearance.AppCompat.Headline">
        <item name="android:fontFamily">@font/google_sans</item>
        <item name="android:textColor">?android:textColorPrimary</item>
    </style>

    <style name="TextAppearance.OPLApps.OnboardingDeck" parent="@style/TextAppearance.AppCompat.Body1">
        <item name="android:fontFamily">@font/google_sans</item>
        <item name="android:textColor">?android:textColorSecondary</item>
        <item name="android:textSize">18sp</item>
        <item name="android:lineSpacingMultiplier">@dimen/text_line_spacing</item>
    </style>

    <style name="TextAppearance.OPLApps.Subhead" parent="@style/TextAppearance.OPLApps.Body">
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">?android:textColorSecondary</item>
        <item name="android:textAllCaps">true</item>
    </style>

    <style name="TextAppearance.OPLApps.SpeakerTitle" parent="@style/TextAppearance.OPLApps.SessionTitle">
        <item name="android:textColor">?colorPrimary</item>
    </style>

    <style name="TextApparance.OPLApps.Map.Label" parent="TextAppearance.AppCompat.Body1">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">#FFF</item>
        <item name="android:shadowColor">#FF000000</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">3</item>
    </style>

    <style name="TextAppearance.OPLApps.Map.InfoTitle" parent="@style/TextAppearance.AppCompat.Title">
        <item name="android:textSize">18sp</item>
        <item name="android:fontFamily">@font/google_sans</item>
        <item name="android:textColor">@color/collapsing_section</item>
    </style>

    <style name="TextAppearance.OPLApps.FadingSnackbarAction" parent="@style/TextAppearance.AppCompat.Button">
        <item name="android:textColor">@color/md_teal_500</item>
        <item name="android:textStyle">bold</item>
        <item name="android:fontFamily">@font/google_sans</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="TextAppearance.OPLApps.Links" parent="@style/TextAppearance.OPLApps.Body">
        <item name="android:textColor">?android:textColorPrimary</item>
        <item name="android:textColorLink">?colorPrimary</item>
        <item name="android:fontFamily">@font/google_sans</item>
    </style>

    <style name="Widget.OPLApps" parent="@android:style/Widget" />

    <style name="Widget.OPLApps.BottomNav" parent="@style/Widget.MaterialComponents.BottomNavigationView.Colored">
        <item name="itemIconTint">@color/bottom_nav_item</item>
        <item name="itemTextAppearanceActive">@style/TextAppearance.OPLApps.BottomNavigationView.Active</item>
        <item name="itemTextAppearanceInactive">@style/TextAppearance.OPLApps.BottomNavigationView</item>
        <item name="labelVisibilityMode">selected</item>
        <item name="android:theme">@style/ThemeOverlay.MaterialComponents.Dark</item>
    </style>

    <style name="Widget.OPLApps.Tabs" parent="@style/Widget.MaterialComponents.TabLayout">
        <item name="tabTextAppearance">@style/TextAppearance.OPLApps.Tab</item>
        <item name="tabIndicator">@drawable/tab_indicator</item>
    </style>

    <style name="Widget.OPLApps.Tag">
        <item name="android:gravity">center_vertical</item>
        <item name="android:lines">1</item>
        <item name="android:textAppearance">@style/TextAppearance.OPLApps.Tag</item>
    </style>

    <style name="Widget.OPLApps.Tag.Inline">
        <item name="android:drawablePadding">@dimen/spacing_micro</item>
        <item name="android:drawableStart">@drawable/tag_dot</item>
    </style>

    <style name="Widget.OPLApps.Tag.Chip">
        <item name="android:background">@drawable/tag_filled</item>
        <item name="android:paddingLeft">@dimen/tag_hz_padding</item>
        <item name="android:paddingRight">@dimen/tag_hz_padding</item>
        <item name="android:paddingTop">@dimen/tag_vert_padding</item>
        <item name="android:paddingBottom">@dimen/tag_vert_padding</item>
        <item name="android:textColor">?android:textColorPrimary</item>
    </style>

    <style name="Widget.OPLApps.EventFilters">
        <item name="android:padding">@dimen/spacing_normal</item>
        <item name="android:strokeColor">?android:textColorTertiary</item>
        <item name="android:textColor">?android:textColorPrimary</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="Widget.OPLApps.TimeHeaders">
        <item name="android:width">@dimen/session_keyline</item>
        <item name="android:paddingTop">@dimen/time_header_padding_top</item>
        <item name="android:textColor">?colorPrimary</item>
        <item name="android:fontFamily">@font/google_sans</item>
    </style>

    <style name="Widget.OPLApps.DateHeaders">
        <item name="android:width">@dimen/session_keyline</item>
        <item name="android:paddingTop">@dimen/spacing_normal</item>
        <item name="android:textColor">?colorPrimary</item>
        <item name="android:fontFamily">@font/google_sans</item>
    </style>

    <style name="Widget.OPLApps.RaisedButton" parent="@style/Widget.AppCompat.Button.Colored">
        <item name="android:fontFamily">@font/google_sans</item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">@drawable/raised_button_background</item>
        <item name="android:letterSpacing">0.025</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:paddingLeft">24dp</item>
        <item name="android:paddingRight">24dp</item>
        <item name="android:paddingTop">12dp</item>
        <item name="android:paddingBottom">12dp</item>
    </style>

    <style name="Widget.OPLApps.BorderlessButton" parent="@style/Widget.MaterialComponents.Button.TextButton">
        <item name="android:fontFamily">@font/google_sans</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">?colorPrimary</item>
        <item name="android:letterSpacing">0.025</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:paddingLeft">24dp</item>
        <item name="android:paddingRight">24dp</item>
        <item name="android:paddingTop">@dimen/padding_borderless_button</item>
        <item name="android:paddingBottom">@dimen/padding_borderless_button</item>
    </style>

    <style name="Widget.OPLApps.OnboardingDeck" parent="@android:style/Widget.Material.TextView">
        <item name="android:textAppearance">@style/TextAppearance.OPLApps.OnboardingDeck</item>
        <item name="android:lineSpacingMultiplier">@dimen/text_line_spacing</item>
        <item name="android:textAlignment">center</item>
    </style>

    <style name="Widget.OPLApps.TextView" parent="@android:style/Widget.Material.TextView">
        <item name="android:textAppearance">@style/TextAppearance.OPLApps.Body</item>
        <item name="android:lineSpacingMultiplier">@dimen/text_line_spacing</item>
    </style>

    <style name="Widget.OPLApps.SessionDetailTitle">
        <item name="android:gravity">start|bottom</item>
        <item name="android:textAppearance">@style/TextAppearance.OPLApps.SessionTitle.Display</item>
        <item name="android:breakStrategy" tools:targetApi="m">balanced</item>
        <item name="android:lineSpacingMultiplier">1</item>
        <item name="autoSizeTextType">uniform</item>
        <item name="autoSizeMaxTextSize">34sp</item>
    </style>

    <style name="Widget.OPLApps.Dialog">
        <item name="android:background">@drawable/dialog_bg</item>
        <item name="android:elevation">@dimen/dialog_elevation</item>
    </style>

    <style name="TextAppearance.OPLApps.CollapsibleCard" parent="@style/TextAppearance.AppCompat.Title">
        <item name="android:textSize">16sp</item>
        <item name="android:fontFamily">@font/google_sans</item>
        <item name="android:textColor">@color/collapsing_section</item>
    </style>

    <style name="TextAppearance.OPLApps.InfoEventTitle" parent="@style/TextAppearance.AppCompat.Title">
        <item name="android:textSize">24sp</item>
        <item name="android:fontFamily">@font/google_sans</item>
        <item name="android:textColor">@color/md_grey_800</item>
    </style>

    <style name="TextAppearance.OPLApps.InfoEventDescription" parent="@style/TextAppearance.AppCompat.Body1">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/md_grey_800</item>
        <item name="android:lineSpacingExtra">8sp</item>
    </style>

    <style name="TextAppearance.OPLApps.InfoEventDescription.AppCallout">
        <item name="android:textColor">?android:textColorLink</item>
    </style>

    <style name="TextAppearance.OPLApps.SettingsSwitch" parent="@style/TextAppearance.AppCompat.Body1">
        <item name="android:background">?selectableItemBackground</item>
        <item name="android:padding">@dimen/margin_normal</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/md_grey_800</item>
    </style>

    <style name="TextAppearance.OPLApps.AboutButtons" parent="@style/TextAppearance.MaterialComponents.Button">
        <item name="android:fontFamily">@font/google_sans</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="TextAppearance.OPLApps.DialogTitle" parent="@style/TextAppearance.AppCompat.Title">
        <item name="android:textSize">22sp</item>
        <item name="android:fontFamily">@font/google_sans</item>
        <item name="android:gravity">start</item>
    </style>

    <style name="TextAppearance.OPLApps.DialogContent" parent="@style/TextAppearance.AppCompat.Body1">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">?android:textColorSecondary</item>
        <item name="android:gravity">start</item>
    </style>

    <style name="Theme.OPLApps.Dialog" parent="@style/Theme.MaterialComponents.Light.Dialog">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:textViewStyle">@style/Widget.OPLApps.TextView</item>
    </style>

    <style name="Widget.OPLApps.AboutButtons" parent="@style/Widget.MaterialComponents.Button.TextButton">
        <item name="android:textAppearance">@style/TextAppearance.OPLApps.AboutButtons</item>
        <item name="android:gravity">start|center_vertical</item>
        <item name="android:paddingLeft">@dimen/margin_normal</item>
        <item name="android:paddingRight">@dimen/margin_normal</item>
    </style>

    <!-- STYLES -->
    <style name="MdTheme" parent="Theme.MaterialComponents.Light">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:colorBackground">@color/background</item>
        <item name="colorError">#F44336</item>
        <item name="android:windowContentTransitions">true</item>
    </style>

    <style name="MdTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:statusBarColor">@color/colorPrimaryDark</item>
    </style>

    <style name="MdThemeMoi" parent="MdTheme">
        <item name="android:statusBarColor">@color/colorPrimaryDark</item>
        <item name="android:windowLightStatusBar" tools:targetApi="m">true</item>
        <item name="colorControlActivated">@color/colorControlActivated</item>
        <item name="android:textColorPrimary">@color/textColorPrimary</item>
        <item name="android:windowTranslucentNavigation">true</item>
        <item name="colorButtonNormal">@color/colorPrimary</item>
        <item name="colorControlNormal">@color/colorPrimary</item>
        <item name="colorControlHighlight">@color/colorPrimaryDark</item>
        <item name="textAppearanceHeadline1">@style/TextAppearance.MaterialComponents.Headline1</item>
        <item name="textAppearanceHeadline2">@style/TextAppearance.MaterialComponents.Headline2</item>
        <item name="textAppearanceHeadline3">@style/TextAppearance.MaterialComponents.Headline3</item>
        <item name="textAppearanceHeadline4">@style/TextAppearance.MaterialComponents.Headline4</item>
        <item name="textAppearanceHeadline5">@style/TextAppearance.MaterialComponents.Headline5</item>
        <item name="textAppearanceHeadline6">@style/TextAppearance.MaterialComponents.Headline6</item>
        <item name="textAppearanceSubtitle1">@style/TextAppearance.MaterialComponents.Subtitle1</item>
        <item name="textAppearanceSubtitle2">@style/TextAppearance.MaterialComponents.Subtitle2</item>
        <item name="textAppearanceBody1">@style/TextAppearance.MaterialComponents.Body1</item>
        <item name="textAppearanceBody2">@style/TextAppearance.MaterialComponents.Body2</item>
        <item name="textAppearanceCaption">@style/TextAppearance.MaterialComponents.Caption</item>
        <item name="textAppearanceButton">@style/TextAppearance.MaterialComponents.Button</item>
        <item name="textAppearanceOverline">@style/TextAppearance.MaterialComponents.Overline</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textStyle">normal</item>
        <item name="android:text">sans-serif</item>
        <item name="dialogTheme">@style/Mdheme.Dialog.Green</item>
        <item name="alertDialogTheme">@style/Mdheme.Dialog.Alert.Green</item>
    </style>

    <style name="Mdheme.Dialog.Green" parent="Theme.AppCompat.Light.Dialog">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <!-- Green Alert Dialog Theme -->
    <style name="Mdheme.Dialog.Alert.Green" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

